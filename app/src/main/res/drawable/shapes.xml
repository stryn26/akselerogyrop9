<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
<!--        membuat bentuk oval -->
        <shape android:shape="oval">

<!--            memberikan warna dan ketebalan ke garis tepi bentuk -->
            <stroke android:color="#29C5F6" android:width="3dp"/>

<!--            memberikan lebar ke bentuk yang dibuat -->
            <size android:width="120dp" android:height="120dp"/>
        </shape>
    </item>

</selector>
